<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <add key="BotId" value="__BOTID__" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  <add key="MicrosoftAppId" value="__MICROSOFTAPPID__" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  <add key="MicrosoftAppPassword" value="__MICROSOFTAPPPASSWORD__" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>